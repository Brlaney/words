                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "{it}hurt{/it} by their callous remarks"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}damage{/sc} suggests injury that lowers value or impairs usefulness. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "a table {it}damaged{/it} in shipping"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}impair{/sc} suggests a making less complete or efficient by deterioration or diminution. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "years of smoking had {it}impaired{/it} his health"
                            }
                        ]
                    ],
                    [
                        "text",
                        " {sc}mar{/sc} applies to injury that spoils perfection (as of a surface) or causes disfigurement. "
                    ],
                    [
                        "vis",
                        [
                            {
                                "t": "the text is {it}marred{/it} by many typos"
                            }
                        ]
                    ]
                ]
