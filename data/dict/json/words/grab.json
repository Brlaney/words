        ],
        "def": [
            {
                "sseq": [
                    [
                        [
                            "sense",
                            {
                                "dt": [
                                    [
                                        "text",
                                        "{bc}the greedy pursuit of an opportunity for making money especially when done without regard for ethics, concerns, or consequences {bc}{sx|money grab||} "
                                    ],
                                    [
                                        "vis",
                                        [
                                            {
                                                "t": "Some Republican leaders have also said any attempt to undo the state's flat tax amounts to a {wi}cash grab{/wi}.",
                                                "aq": {
                                                    "auth": "Steve LeBlanc"
                                                }
                                            },
                                            {
                                                "t": "\u2026 money generated by single-use plastic going onto the market is being swallowed up in a {wi}cash grab{/wi} \u2026 with little or no accountability or strategy underpinning it.",
                                                "aq": {
                                                    "auth": "Adam Leyland"
                                                }
                                            }
                                        ]
                                    ]
                                ],
                                "sdsense": {
                                    "sd": "also",
                                    "dt": [
                                        [
                                            "text",
                                            "{bc}something (such as a product or service) used for a cash grab "
                                        ],
                                        [
                                            "vis",
                                            [
                                                {
                                                    "t": "HBO announced a new Harry Potter TV series, and it is an obvious {wi}cash grab{/wi} that is insulting to true fans.",
                                                    "aq": {
                                                        "auth": "Victor Lana"
                                                    }
                                                },
                                                {
                                                    "t": "But is it good enough to satisfy discerning taco-loving hipsters or is it simply a shameless, cultural appropriating {wi}cash grab{/wi}?",
                                                    "aq": {
                                                        "auth": "Joey Morona"
                                                    }
                                                },
                                                {
                                                    "t": "It might be more feasible to just give up on college football being anything but a soulless {wi}cash grab{/wi} \u2026",
                                                    "aq": {
                                                        "auth": "David Schraub"
                                                    }
                                                }
                                            ]
                                        ]
                                    ]
                                }
                            }
                        ]
                    ]
                ]
            }
        ],
        "date": "1922",
        "shortdef": [
            "the greedy pursuit of an opportunity for making money especially when done without regard for ethics, concerns, or consequences : money grab; also : something (such as a product or service) used for a cash grab"
        ]
    },
