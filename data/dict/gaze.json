                                        "{bc}a fixed intent look "
                                    ],
                                    [
                                        "vis",
                                        [
                                            {
                                                "t": "a steady {wi}gaze{/wi}"
                                            },
                                            {
                                                "t": "He fixed his {wi}gaze{/wi} out the window."
                                            },
                                            {
                                                "t": "She lowered/dropped/raised her {wi}gaze{/wi}."
                                            },
                                            {
                                                "t": "I {phrase}met his gaze{/phrase}. {gloss}=I looked back at him.{/gloss}"
                                            }
                                        ]
                                    ]
                                ]
